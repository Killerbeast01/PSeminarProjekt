<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="300.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="67.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/model/arrow_turn_left.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="390.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/model/arrow_turn_right.png" />
         </image>
      </ImageView>
      <Button fx:id="btn_old_school" layoutX="72.0" layoutY="63.0" mnemonicParsing="false" onAction="#btn_old_school" prefHeight="30.0" prefWidth="150.0" text="Altes Schulgebäude">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_recent_school" layoutX="380.0" layoutY="63.0" mnemonicParsing="false" onAction="#btn_recent_school" prefHeight="30.0" prefWidth="170.0" text="Aktuelles Schulgebäude">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_otto" layoutX="258.0" layoutY="63.0" mnemonicParsing="false" onAction="#btn_otto" prefHeight="30.0" prefWidth="90.0" text="Otto Hahn">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_rotate_reset" layoutX="245.0" layoutY="170.0" mnemonicParsing="false" onAction="#btn_rotate_reset" text="Zentrieren">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="btn_rotate_left" layoutX="89.0" layoutY="131.0" mnemonicParsing="false" onAction="#btn_rotate_left" opacity="0.0" prefHeight="100.0" prefWidth="100.0" />
      <Button fx:id="btn_rotate_right" layoutX="417.0" layoutY="130.0" mnemonicParsing="false" onAction="#btn_rotate_right" opacity="0.0" prefHeight="100.0" prefWidth="100.0" />
   </children>
</AnchorPane>
