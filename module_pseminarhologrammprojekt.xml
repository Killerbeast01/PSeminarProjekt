<?xml version="1.0" encoding="UTF-8"?>
<project name="module_pseminarhologrammprojekt" default="compile.module.pseminarhologrammprojekt">
  <dirname property="module.pseminarhologrammprojekt.basedir" file="${ant.file.module_pseminarhologrammprojekt}"/>
  
  <property name="module.jdk.home.pseminarhologrammprojekt" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.pseminarhologrammprojekt" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.pseminarhologrammprojekt" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.pseminarhologrammprojekt" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="pseminarhologrammprojekt.output.dir" value="${module.pseminarhologrammprojekt.basedir}/out/production/PSeminarHologrammProjekt"/>
  <property name="pseminarhologrammprojekt.testoutput.dir" value="${module.pseminarhologrammprojekt.basedir}/out/test/PSeminarHologrammProjekt"/>
  
  <path id="pseminarhologrammprojekt.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="pseminarhologrammprojekt.module.production.classpath">
    <path refid="${module.jdk.classpath.pseminarhologrammprojekt}"/>
  </path>
  
  <path id="pseminarhologrammprojekt.runtime.production.module.classpath">
    <pathelement location="${pseminarhologrammprojekt.output.dir}"/>
  </path>
  
  <path id="pseminarhologrammprojekt.module.classpath">
    <path refid="${module.jdk.classpath.pseminarhologrammprojekt}"/>
    <pathelement location="${pseminarhologrammprojekt.output.dir}"/>
  </path>
  
  <path id="pseminarhologrammprojekt.runtime.module.classpath">
    <pathelement location="${pseminarhologrammprojekt.testoutput.dir}"/>
    <pathelement location="${pseminarhologrammprojekt.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.pseminarhologrammprojekt">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.pseminarhologrammprojekt">
    <patternset refid="excluded.from.module.pseminarhologrammprojekt"/>
  </patternset>
  
  <path id="pseminarhologrammprojekt.module.sourcepath">
    <dirset dir="${module.pseminarhologrammprojekt.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.pseminarhologrammprojekt" depends="compile.module.pseminarhologrammprojekt.production,compile.module.pseminarhologrammprojekt.tests" description="Compile module PSeminarHologrammProjekt"/>
  
  <target name="compile.module.pseminarhologrammprojekt.production" depends="register.custom.compilers" description="Compile module PSeminarHologrammProjekt; production classes">
    <mkdir dir="${pseminarhologrammprojekt.output.dir}"/>
    <javac2 destdir="${pseminarhologrammprojekt.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pseminarhologrammprojekt}/javac">
      <compilerarg line="${compiler.args.pseminarhologrammprojekt}"/>
      <bootclasspath refid="pseminarhologrammprojekt.module.bootclasspath"/>
      <classpath refid="pseminarhologrammprojekt.module.production.classpath"/>
      <src refid="pseminarhologrammprojekt.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.pseminarhologrammprojekt"/>
    </javac2>
    
    <copy todir="${pseminarhologrammprojekt.output.dir}">
      <fileset dir="${module.pseminarhologrammprojekt.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.pseminarhologrammprojekt.tests" depends="register.custom.compilers,compile.module.pseminarhologrammprojekt.production" description="compile module PSeminarHologrammProjekt; test classes" unless="skip.tests"/>
  
  <target name="clean.module.pseminarhologrammprojekt" description="cleanup module">
    <delete dir="${pseminarhologrammprojekt.output.dir}"/>
    <delete dir="${pseminarhologrammprojekt.testoutput.dir}"/>
  </target>
</project>